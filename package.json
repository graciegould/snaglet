{
  "name": "snaglet",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=snaglet-server",
    "build": "npm run build --workspaces --if-present",
    "start": "npm start --workspace=snaglet-server",
    "bootstrap": "npm run bootstrap --workspace=snaglet-server",
    "lint": "npm run lint --workspaces --if-present"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "sass": "^1.77.2",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
